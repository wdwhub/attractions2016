<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% @attractions.each do |attraction| %>
      <tr>
        <td><%= link_to attraction.name, attraction_path(attraction) %></td>
        <td><%= attraction.what_it_is %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

